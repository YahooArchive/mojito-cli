{
  "name": "mojito-cli",
  "version": "0.0.0",
  "description": "command line utilities for mojito",
  "bin": {
    "mo": "bin/mojito"
  },
  "preferGlobal": true,
  "dependencies": {
    "mojito-create": "*",
    "npmlog": "0.0.2",
    "optimist": "~0.3.5"
  },
  "devDependencies": {
    "tape": "~0.3.0",
    "istanbul": "~0.1.32"
  },
  "scripts": {
    "preinstall": "\n#\n# Note, if mojito is installed globally, uninstall it first to use this package.\n#",
    "test": "tape tests/*.js | tee tests/results.tap",
    "cover": "istanbul cover --dir artifacts/coverage node_modules/.bin/tape tests/*.js",
    "plato": "plato -d artifacts/plato *.js"
  },
  "repository": "git://github.com/isao/mojito-cli.git",
  "author": "Isao Yagi <isao@yahoo-inc.com>",
  "license": "MIT",
  "readmeFilename": "README.md"
}
