{
  "name": "mojito-cli",
  "version": "0.0.2",
  "description": "The mojito-cli package provides command line helpers for Mojito developers.",
  "bin": {
    "mojito": "bin/mojito"
  },
  "main": "cli.js",
  "preferGlobal": true,
  "dependencies": {
    "mojito-build": "~0.0.0",
    "mojito-create": "~0.0.2",
    "npmlog": "0.0.2",
    "nopt": "~2.1.1"
  },
  "devDependencies": {
    "tap": "~0.4.1"
  },
  "engines": {
    "node": ">0.8",
    "npm": ">1.1"
  },
  "scripts": {
    "lint": "jshint {.,lib,commands}/*.js",
    "test": "node tests",
    "cover": "istanbul cover --dir artifacts/coverage tests",
    "clean": "git clean -dfx {.,tests}/artifacts",
    "preinstall": "scripts/preinstall.sh"
  },
  "repository": "git://github.com/yahoo/mojito-cli.git",
  "bugs": "https://github.com/yahoo/mojito-cli/issues",
  "author": "Isao Yagi <isao@yahoo-inc.com>",
  "license": "BSD",
  "readmeFilename": "README.md"
}
