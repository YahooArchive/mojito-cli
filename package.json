{
  "name": "mojito-cli",
  "version": "0.0.0",
  "description": "command line utilities for mojito",
  "bin": {
    "mo": "bin/mojito"
  },
  "preferGlobal": true,
  "dependencies": {
    "mojito-create": "*",
    "optimist": "~0.3.5"
  },
  "bundledDependecies": [
    "mojito-create"
  ],
  "devDependencies": {
    "tape": "~0.3.0"
  },
  "scripts": {
    "preinstall": "\n# Note, if mojito is installed globally, uninstall it first to use this package.",
    "test": "tape tests/*.js",
    "cover": "istanbul cover --dir reports/coverage node_modules/.bin/tape tests/*.js",
    "plato": "plato -d reports/plato *.js"
  },
  "repository": "git://github.com/isao/mojito-cli.git",
  "author": "Isao Yagi <isao@yahoo-inc.com>",
  "license": "MIT",
  "readmeFilename": "README.md"
}
