var cli = require('../');

cli.run(process.argv.slice(2), function done(err, msg) {
    var out;
    if(err) {
    	out = err instanceof Error ? err.message : err;
    } else if(msg) {
    	cli.log.info('ok', msg);
    }
});

//process.on('uncaughtexception', );